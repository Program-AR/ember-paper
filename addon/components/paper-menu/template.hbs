<BasicDropdown
  @disabled={{@disabled}}
  @triggerComponent={{component "paper-menu/trigger"}}
  @contentComponent={{component "paper-menu/content"}}
  @calculatePosition={{this.calculatePosition}}
  @onClose={{this.close}}
  @onOpen={{this.open}}
  ...attributes
  as |dd|>
  {{yield (hash
    trigger=dd.Trigger
    content=dd.Content
  )}}
</BasicDropdown>